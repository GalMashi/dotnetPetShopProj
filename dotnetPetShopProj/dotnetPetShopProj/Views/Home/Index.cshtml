@model IEnumerable<dotnetPetShopProj.Models.Animal>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Pages/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<table>
    @foreach (var CurrentAnimal in @Model)
    {
        <tr>
            <td>@CurrentAnimal.AnimalName @*&nbsp;&nbsp;*@</td>
        </tr>
    }
</table>
